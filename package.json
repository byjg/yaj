{
  "name": "yaj",
  "version": "0.1.8",
  "description": "Yaj.js - Yet another jQuery replacement",
  "main": "yaj.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest --coverage",
    "minify-yaj": "./node_modules/.bin/uglifyjs --compress 'drop_console,drop_debugger' --comments '/^!/i' --source-map --mangle -o yaj.min.js yaj.js",
    "minify": "npm run minify-yaj",
    "publish-patch": "npm version patch && git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/byjg/yaj.git"
  },
  "keywords": [
    "jquery",
    "jquery-replacement",
    "polyfill"
  ],
  "author": "João Gilberto Magalhães",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/byjg/yaj/issues"
  },
  "dependencies": {
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "uglify-js": "^3.0.24",
    "jest": "^20.0.1"
  }
}
